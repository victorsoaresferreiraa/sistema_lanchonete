modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["cairo", "chrpath", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "pkg-config", "python312Full", "python312Packages.tkinter", "qhull", "scons", "tcl", "tigervnc", "tk", "xorg.xhost", "xorg.xinit", "xorg.xorgserver", "zlib"]

[workflows]
